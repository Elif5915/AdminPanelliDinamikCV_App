@using AdminPanelliDinamikCV_App.Models.Entity
@model List<About>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>About Me Edit Page</h2>
<br />
<form method="post">

    @foreach (var item in Model)
    {
        <label>Name</label>
        <input type="text" class="form-control" name="Name" value="@item.Name" />
        <br />
        <label>Surname</label>
        <input type="text" class="form-control" name="Surname" value="@item.Surname" />
        <br />
        <label>Address</label>
        <input type="text" class="form-control" name="Address" value="@item.Address" />
        <br />
        <label>Phone</label>
        <input type="text" class="form-control" name="Phone" value="@item.Phone" />
        <br />
        <label>Email</label>
        <input type="text" class="form-control" name="Email" value="@item.Email" />
        <br />
        <label>Comment</label>
        <input type="text" class="form-control" name="Comment" value="@item.Comment" />
        <br />
        <label>Image</label>
        <input type="text" class="form-control" name="Image" value="@item.Image" />

    }
    <br /><br />
    <button class="btn btn-info">Update</button>

</form>