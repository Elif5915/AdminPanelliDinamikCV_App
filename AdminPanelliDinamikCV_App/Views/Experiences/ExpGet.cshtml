@model AdminPanelliDinamikCV_App.Models.Entity.Experiences

@{
    ViewBag.Title = "ExpGet";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Update Experience Page</h4>
<br />

<form method="post">
    @Html.Label("ID")
    @Html.TextBoxFor(x => x.Id, new { @class = "form-control" })
    <br />
    @Html.Label("Title")
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />
    @Html.Label("SubHead")
    @Html.TextBoxFor(x => x.Subhead, new { @class = "form-control" })
    <br />
    @Html.Label("Comment")
    @Html.TextAreaFor(x => x.Comment, new { @class = "form-control" })
    <br />
    @Html.Label("Date")
    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Update</button>
</form>