@model AdminPanelliDinamikCV_App.Models.Entity.SocialMedia

@{
    ViewBag.Title = "updateSocial";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Social Media Update Page</h4>
<br />

<form method="post">
    @Html.Label("Name")
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("Icon")
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
    <br />

    <button class="btn btn-success">Update</button>
</form>