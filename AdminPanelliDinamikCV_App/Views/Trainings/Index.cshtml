@using AdminPanelliDinamikCV_App.Models.Entity
@model List<Trainings>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Trainings List</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>SubTitle</th>
        <th>SubTitle2</th>
        <th>Gno</th>
        <th>Date</th>
        <th></th>

    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>
        @item.Id
    </td>
    <td>
        @item.Title
    </td>
    <td>
        @item.SubTitle
    </td>
    <td>
        @item.SubTitle2
    </td>
    <td>
        @item.Gno
    </td>
    <td>
        @item.Date
    </td>
    <td>
        <a href="/Trainings/EduDelete/@item.Id" class="btn btn-danger btn-sm">Delete</a>
        <a href="/Trainings/UpdateEdu/@item.Id" class="btn btn-info btn-sm">Update</a>
    </td>

</tr>
    }
</table>
<a href="/Trainings/addEdu/" class="btn btn-primary">Add New Trainings</a>



